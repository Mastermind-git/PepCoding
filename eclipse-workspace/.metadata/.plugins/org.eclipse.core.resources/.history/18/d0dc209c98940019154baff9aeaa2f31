package HashMapHeap;

import java.util.*;


public class two_sum {
	 public static ArrayList<Integer> twoSum(ArrayList<Integer> nums, int target) {
	 	 //Write your code here
		 HashMap<Integer, Integer> indx = new HashMap<Integer, Integer>();
		 for(int i =0;i<nums.size();i++)
		 {
			 indx.put(i, nums.get(i));
			 System.out.println(i+" "+nums.get(i));
		 }
		 Collections.sort(nums);
		 int i = 0 , j = nums.size();
		 while(i<j)
		 {
			 int sum = nums.get(i)+nums.get(j);
			 if(sum==target) {
				 System.out.println(indx.ge);
			 }
		 }
		 return null;
	 }
	 
	 	 //Don't make any changes here.
	 	 public static void main(String[] args) {
	 
	 	 	 Scanner s = new Scanner(System.in);
	 	 	 ArrayList<Integer> nums = new ArrayList<Integer>();
	 	 	 int n = s.nextInt();
	 	 	 for (int i = 0; i < n; i++) {
	 	 	 	 nums.add(s.nextInt());
	 	 	 }
	 	 	 int target = s.nextInt();
	 	 	 System.out.println(twoSum(nums, target));
	 
	 	 }
	 	 
}
