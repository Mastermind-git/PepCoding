package ArraysString;

import java.util.Scanner;

public class Nesting_Array {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int arr[] = new int[n];
		for(int i=0;i<n;i++)
		arr[i] = s.nextInt();
		int visited[] = new int[n];
		for(int i=0;i<n;i++)
			visited[i] = 0;
		int max=0;
		for(int i=0;i<n;i++)
		{
			int length=1;
			int j =arr[i];
			while(visited[j]==0)
			{
				 visited[j] = -1;
				  j = arr[j]-1;
				
				 System.out.println(arr[j]+"<<<<<<<");
				 
			}
			System.out.println(":::");
		}
		
	}
}
