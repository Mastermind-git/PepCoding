package HashMapHeap;

import java.io.*;
import java.util.*;
public class distinct_elements_in_a_stream {

	 public static void print(String[] arr) throws IOException {
	 	 //  Write your code here
	 	 	 	 HashMap<Character,Integer> hm = new HashMap<Character, Integer>();
	 	 	 	 for (int i = 0; i < arr.length; i++) {
					
						if(arr[i].charAt(0)=='A')
					{
						if(hm.containsKey(arr[i].charAt(i)))
							hm.put(arr[i].charAt(2),hm.get(arr[i].charAt(i))+1);
						else
							hm.put(arr[i].charAt(2),1);
					}
						else if(arr[i].charAt(0)=='R')
						{
							if(hm.containsKey(arr[i].charAt(i)))
								{hm.put(arr[i].charAt(2),hm.get(arr[i].charAt(i))-1);
								if(hm.get(arr[i].charAt(i))==0) 
										hm.remove(arr[i].charAt(2));
								}
							
						}
				System.out.println(hm.size());
				
				}
	 	 }
	 	 //Don't make any changes here
	 	 public static void main(String[] args) throws IOException {
	 	 	 BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	 	 	 int n = Integer.parseInt(br.readLine());
	 	 	 String[] arr = new String[n];
	 	 	 for (int i = 0; i < n; i++) {
	 	 	 	 arr[i] = br.readLine();
	 	 	 }
	 
	 	 	 print(arr);
	 
	 	 }
}
