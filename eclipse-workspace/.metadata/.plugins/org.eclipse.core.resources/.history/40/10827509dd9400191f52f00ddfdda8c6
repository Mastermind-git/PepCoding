package HashMapHeap;

import java.util.*;

public class uncommon_characters {

	 public static void uncommon(String str1, String str2) {
	 	 //Write your code here	 
		 HashSet<Character> hm1 = new  HashSet<Character>();
		 HashSet<Character> hm2 = new  HashSet<Character>();
		 for(int i=0;i<str1.length();i++)
			 	hm1.add(str1.charAt(i));
		 for(int i=0;i<str2.length();i++)
			 	hm2.add(str2.charAt(i));
		 Iterator<Character> it = hm2.iterator();
		 while(it.hasNext())
		 {
			 char c  = it.next();
			 System.out.println(c);
			 if(hm1.contains(c))
			 {
				 hm1.remove(c);
				 hm2.remove(c);
			 }
		 }
		 char ch [] =new char[hm1.size()+hm2.size()];
		 it=hm1.iterator();
		 int i=0;
		 while(it.hasNext())
			 {
			 ch[i] = it.next();
			 i++;
			 };
		it = hm2.iterator();
		while(it.hasNext())
		{
			ch[i]=it.next();
			i++;
		}
		Arrays.sort(ch);
		System.out.println(ch.toString());
	 	 }
	 
	 	 //Don't make any changes here
	 	 public static void main(String[] args) {
	 	 	 Scanner sc = new Scanner(System.in);
	 	 	 String str1 = sc.next();
	 
	 	 	 String str2 = sc.next();
	 
	 	 	 uncommon(str1, str2);
	 
	 	 }
}
